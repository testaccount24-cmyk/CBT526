/* REXX THE WONDER DOG */
signal off error
arg acid newpass
rcode = 0
address tso
say "tss rem("acid") asuspend "
"tss rem("acid") asuspend "
rcode = rcode + rc
say "tss rem("acid") suspend "
"tss rem("acid") suspend "
rcode = rcode + rc
say "tss rep("acid") password("newpass")"
"tss rep("acid") password("newpass")"
rcode = rcode + rc
exit 0
exit rcode
