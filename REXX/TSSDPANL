/*REXX*/
/** rexx                                                 **/
/**********************************************************/
/** BASELINE BASELINE BASELINE BASELINE BASELINE BASELINE**/
/**********************************************************/
/** EXEC NAME        : TSSDPANL                          **/
/** LAST MODIFIED    : 23 OCT 95                         **/
/**----------------INPUT REQUIRED -----------------------**/
/** PASSED ARGS        VALID VALUES                      **/
/** VARIABLES THE NAMES OF THE VARIABLES ON THE PANEL    **/
/**----------------OUTPUT PRODUCED-----------------------**/
/** VALID CONDITION CODES - 00 16                        **/
/** ISPF VARIABLES        - VARIABLE DEPENDING ON INPUT  **/
/**                                                      **/
/**----------------REQUIREMENTS--------------------------**/
/** CAN RUN STANDALONE : YES.(NOTE VGET VARIABLES)       **/
/** ISPF ENVIRONMENT   : YES                             **/
/** EXECS CALLED WITHIN THIS EXEC : NONE                  */
/**                                                      **/
/**********************************************************/
ADDRESS ISPEXEC
ARG THEVARS
BLANKS = "      "
THEVARS = TRANSLATE(THEVARS," ",".")
VARNUM = WORDS(THEVARS)
PARSE VAR THEVARS W.1 W.2 W.3 W.4 W.5 W.6 W.7 W.8 W.9 W.10 W.11,
       W.12 W.13 W.14 W.15 W.16 W.17 W.18 W.19 W.20
CNT = VARNUM + 1
DO T = CNT TO 20
  W.T = "BLANKS"
END
VNAME1  = W.1
VNAME2  = W.2
VNAME3  = W.3
VNAME4  = W.4
VNAME5  = W.5
VNAME6  = W.6
VNAME7  = W.7
VNAME8  = W.8
VNAME9  = W.9
VNAME10 = W.10
VNAME11 = W.11
VNAME12 = W.12
VNAME13 = W.13
VNAME14 = W.14
VNAME15 = W.15
VNAME16 = W.16
VNAME17 = W.17
VNAME18 = W.18
VNAME19 = W.19
VNAME20 = W.20
ADDRESS ISPEXEC "VGET" VNAME1     "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME2     "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME3     "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME4     "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME5     "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME6     "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME7     "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME8     "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME9     "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME10    "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME11    "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME12    "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME13    "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME14    "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME15    "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME16    "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME17    "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME18    "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME19    "PROFILE"
ADDRESS ISPEXEC "VGET" VNAME20    "PROFILE"
VVAL1 = VALUE(VNAME1)
VVAL2 = VALUE(VNAME2)
VVAL3 = VALUE(VNAME3)
VVAL4 = VALUE(VNAME4)
VVAL5 = VALUE(VNAME5)
VVAL6 = VALUE(VNAME6)
VVAL7 = VALUE(VNAME7)
VVAL8 = VALUE(VNAME8)
VVAL9 = VALUE(VNAME9)
VVAL10 = VALUE(VNAME10)
VVAL11 = VALUE(VNAME11)
VVAL12 = VALUE(VNAME12)
VVAL13 = VALUE(VNAME13)
VVAL14 = VALUE(VNAME14)
VVAL15 = VALUE(VNAME15)
VVAL16 = VALUE(VNAME16)
VVAL17 = VALUE(VNAME17)
VVAL18 = VALUE(VNAME18)
VVAL19 = VALUE(VNAME19)
VVAL20 = VALUE(VNAME20)
SIGNAL OFF ERROR
"DISPLAY PANEL(VARPANEL)"
/*************************************************************/
/* THE DOUBLE QUOTES BELOW R  TO PRESERVE THE SINGLE QUOTES  */
/* THE VARIABLES.                                    DAVE    */
/*************************************************************/
INTERPRET VNAME1 '=' '"'VVAL1'"'
INTERPRET VNAME2 '=' '"'VVAL2'"'
INTERPRET VNAME3 '=' '"'VVAL3'"'
INTERPRET VNAME4 '=' '"'VVAL4'"'
INTERPRET VNAME5 '=' '"'VVAL5'"'
INTERPRET VNAME6 '=' '"'VVAL6'"'
INTERPRET VNAME7 '=' '"'VVAL7'"'
INTERPRET VNAME8 '=' '"'VVAL8'"'
INTERPRET VNAME9 '=' '"'VVAL9'"'
INTERPRET VNAME10 '=' '"'VVAL10'"'
INTERPRET VNAME11 '=' '"'VVAL11'"'
INTERPRET VNAME12 '=' '"'VVAL12'"'
INTERPRET VNAME13 '=' '"'VVAL13'"'
INTERPRET VNAME14 '=' '"'VVAL14'"'
INTERPRET VNAME15 '=' '"'VVAL15'"'
INTERPRET VNAME16 '=' '"'VVAL16'"'
INTERPRET VNAME17 '=' '"'VVAL17'"'
INTERPRET VNAME18 '=' '"'VVAL18'"'
INTERPRET VNAME19 '=' '"'VVAL19'"'
INTERPRET VNAME20 '=' '"'VVAL20'"'
"VGET ZAPPLID"
DO P = 1 TO 20
  ADDRESS ISPEXEC "VPUT" W.P "PROFILE"
  X = VALUE(W.P)
  /* SAY W.P "IS" X*/
END
EXIT 0
